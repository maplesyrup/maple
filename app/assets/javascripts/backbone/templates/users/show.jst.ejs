<div class="container">
	<div class="center">
		<div id="user-info-container">
			<img id="avatar" contenteditable="<%= editable %>" src="<%= avatar %>" class="img-polaroid small-photo pull-left">
			<div class="pull-left">
					<%if (name) { %>
				<h2> <%= name %> </h2>
					<%} else { %> 
					<h2> Calvin Fernandez </h2>
					<%}%>
				<hr> 
				<p id="personal-info" contenteditable="<%= editable %>">
					<%= personal_info %>	
				</p>		
			</div>
		</div>
	</div>
</div>
<hr>

<div id="user-select-new-avatar">
</div>

<div class="container-fluid">
	<div class="center">
		<div class="btn-group center-container">
	  	<button class="btn active">Posts</button>
  		<button class="btn">Likes</button>
  		<button class="btn">Following</button>
  		<button class="btn">Followers</button>
		</div>
	</div>
</div>
<div id="user-main-container" class="container-fluid nav-escape-above">

</div>
