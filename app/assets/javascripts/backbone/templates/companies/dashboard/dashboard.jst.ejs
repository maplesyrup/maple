<div class="row-fluid">
  <div class="span8">
    <h2><%= name %> Dashboard</h2>
  </div>
  <div class="span4">
    <select name="date-range" id="filter-date">
      <option value="week" data-range="week">This Week (<%= Date.today().add(-7).days().toString("MMMM dd, yyyy") %> - <%= Date.today().toString("MMMM dd, yyyy") %>)</option>
      <option value="month" data-range="month">This Month (<%= Date.today().add(-1).months().toString("MMMM dd, yyyy") %> - <%= Date.today().toString("MMMM dd, yyyy") %>)</option>
    </select>
  </div>
</div>

<div class="row-fluid">
  <div id="dashboard-graph" class="span12">
    
  </div>
</div>

<div id="dashboard-stats" class="row-fluid">
  
</div>

